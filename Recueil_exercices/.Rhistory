dhs_ipv <- read_csv("../Data/dhs_ipv.csv")
summary(dhs_ipv$beat_goesout)
View(dhs_ipv)
reg1 <- lm(formula = beat_burnfood ~ sec_school + no_media + region, data = dhs_ipv)
summary(reg1)
summary(reg1)
dhs_ipv <- dhs_ipv %>%
mutate(period = factor(if_else(year < 2010, "avant 2010", "après 2010")))
freq(dhs_ipv$period)
reg2 <- lm(formula = beat_burnfood ~ sec_school + period + sec_school:periode + no_media + region, data = dhs_ipv)
reg2 <- lm(formula = beat_burnfood ~ sec_school + period + sec_school:period + no_media + region, data = dhs_ipv)
summary(reg2)
reg2 <- lm(formula = beat_burnfood ~ sec_school + period + no_media:period + no_media + region, data = dhs_ipv)
summary(reg2)
reg3 <- lm(formula = beat_burnfood ~ sec_school + no_media*period + region, data = dhs_ipv)
summary(reg3)
library(huxtable)
